{
  "private": false,
  "nps": 500000,

  "source": "https://github.com/tbreese3/Helios",
  "repo":   "https://github.com/tbreese3/Helios",
  "ref":    "main",

  "build": {
    "path": "scripts/build_helios.sh",
    "compilers": [],
    "cpuflags": ["BMI2", "PEXT"],
    "systems": ["Linux"]
  },

  "exec": "./Helios",
  "bench": "bench",
  "protocol": "uci",

  "test_presets": {
    "default": {
      "base_branch": "main",
      "book_name": "UHO_Lichess_4852_v1.epd",
      "test_bounds": [0.0, 2.0],
      "test_confidence": [0.05, 0.05],
      "win_adj": "movecount=3 score=400",
      "draw_adj": "movenumber=40 movecount=8 score=40",
      "both_options": "Threads=3 Hash=512",
      "both_time_control": "6.0+0.06",
      "workload_size": 32
    }
  },

  "tune_presets": {},
  "datagen_presets": {}
}
